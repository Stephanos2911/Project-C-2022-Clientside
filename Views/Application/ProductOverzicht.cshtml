@model IEnumerable<Product>
@{
    //loads db context into this page
    var _context = ViewBag.Context as ApplicationDbContext;

    //Loads all images of category
    var AllProductImagesofCategory =
      ( from image in _context.Images
        from product in _context.Products
       where product.Place == Model.ToList()[0].Place
       select image).Where(x => x.Type == "Product").ToList();

    List<string> imageDataURL = new List<string>();
    for (int i = 0; i < AllProductImagesofCategory.Count; i++)
    {
        imageDataURL.Add(string.Format("data:image/jpg;base64,{0}", Convert.ToBase64String(AllProductImagesofCategory[i].ImageData)));
    }

}
@*@{
    for (int i = 0; i < imageDataURL.Count; i++)
    {
        <img src="@imageDataURL[i]" asp-append-version="true" height="80" width="80" />
    }
}*@